    2016..2019: spriteset_BR43_GWR_purchase;
    2020..2029: spriteset_BR43_ScotRail_purchase;
        spriteset_BR43_BR_Intercity_Swallow_purchase; }

// FX effects
switch(FEAT_TRAINS, SELF, sw_FX_BR43_Diesel, position_in_vehid_chain % 3) {
    0: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
    1: return visual_effect_and_powered(VISUAL_EFFECT_DIESEL, -1, DISABLE_WAGON_POWER);
        return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER); }

// livery switch
switch(FEAT_TRAINS, SELF, sw_livery_BR43, cargo_subtype) {
    0: spriteset_BR43_BRBlue;
    1: spriteset_BR43_BR_Intercity_Swallow;
    2: spriteset_BR43_BR_Intercity_Executive;   
    3: spriteset_BR43_GNER; }

