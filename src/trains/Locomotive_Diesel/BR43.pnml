    0: spriteset_BR43_BRBlue_REV;
    1: spriteset_BR43_BR_Intercity_Swallow_REV;
    2: spriteset_BR43_BR_Intercity_Executive_REV;
    3: spriteset_BR43_GNER_REV; }

// livery name refit switch
switch(FEAT_TRAINS, SELF, sw_BR43_cargo_subtype_text, cargo_subtype){
	0:	return string(str_BRBlue);
    1:  return string(str_BRInterCitySwallow);
    2:  return string(str_BRInterCityExecutive);
    3:  return string(str_GNER);
       return CB_RESULT_NO_TEXT; }

// graphics switch
switch(FEAT_TRAINS, SELF, sw_graphics_BR43, position_in_consist % 6) {
	1: return sw_livery_BR43;
    4: return sw_livery_BR43_REV;
        spriteset_empty; }

switch(FEAT_TRAINS, SELF, sw_rear_BR43, num_vehs_in_consist > 6 ? position_in_consist_from_end : - 1) {
    1: sw_livery_BR43_REV;
        sw_graphics_BR43; }

// length switch
switch(FEAT_TRAINS, SELF, sw_length_BR43, position_in_articulated_veh % 3) {
	0: 4;
	1: 3;
	2: 4; }

// speed switch
switch(FEAT_TRAINS, SELF, sw_speed_BR43, param_max_speed) {
    0: 125; // operational speed
