// unit definition
item (FEAT_TRAINS, item_BR52, 5200) { // TOPS Number range 52000 - even though they never got them
    property {
        // common properties
        name:                           string(STR_NAME_BR52);
        // variant_group:                  ; // unit ID or item
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(1961,1,1); 
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life: 					40;
        retire_early: 					-10;							
        reliability_decay: 				20;
        
        cargo_allow_refit:               [PASS, GOOD];
        misc_flags: 					bitmask(TRAIN_FLAG_SPRITE_STACK);
        // misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 1;
        loading_speed:					5;
        cost_factor: 					108;					
        running_cost_base:				RUNNING_COST_DIESEL;				
        running_cost_factor: 			90;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          90 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        power:                          2700 hp;
        weight:                         108 ton;
        engine_class:                   ENGINE_CLASS_DIESEL;
        dual_headed:                    0;
        air_drag_coefficient:           0.06;
        tractive_effort_coefficient:    0.27;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_graphics_BR52;
        visual_effect_and_powered:      sw_FX_BR52_Diesel;
        purchase:                       sw_BR52_purchase;
		
		sound_effect:					sw_deltic_sound;

        // internal code
        length:                         sw_length_BR52;
        articulated_part:               sw_articulated_BR52;
        purchase_cargo_capacity:        0;
        cargo_capacity:                 return 0;
        cost_factor:                    return(GetAdjustedCost(36));
        running_cost_factor:            return(GetAdjustedCost(39));
        cargo_subtype_text:             sw_BR52_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;

        // purchase menu text
        additional_text:					return(	string(str_purchase_loco_with_liveries,
													string(str_purchase_type_diesel),
													string(str_BR52_usage),
													string(str_BR52_liveries)));
    }
}