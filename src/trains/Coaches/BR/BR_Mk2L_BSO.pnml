    6: spriteset_BR_Mk2_Late_BSO_ScotRail_Saltire;
}

switch(FEAT_TRAINS, SELF, sw_livery_BR_Mk2L_BSO_REV, cargo_subtype) {
    0: spriteset_BR_Mk2_Late_BSO_BR_Blue_REV;
    1: spriteset_BR_Mk2_Late_BSO_BR_Intercity_REV;
    2: spriteset_BR_Mk2_Late_BSO_BR_ScotRail_REV;
    3: spriteset_BR_Mk2_Late_BSO_Virgin_REV;
    4: spriteset_BR_Mk2_Late_BSO_Anglia_Railways_REV;
    5: spriteset_BR_Mk2_Late_BSO_FGW_Fag_Packet_REV;
    6: spriteset_BR_Mk2_Late_BSO_ScotRail_Saltire_REV;
}

// livery name refit switch
switch(FEAT_TRAINS, SELF, sw_BR_Mk2L_BSO_cargo_subtype_text, cargo_subtype){
	0:	return string(str_BRBlueGrey);
    1:  return string(str_BRInterCity);
    2:  return string(str_BRScotRail);
    3:  return string(str_Virgin);
    4:  return string(str_Anglia);
    5:  return string(str_FGWFag);
    6:  return string(str_ScotRailSaltire);
       return CB_RESULT_NO_TEXT;
}

// random switch
random_switch(FEAT_TRAINS, SELF, random_choose_reverse_BR_Mk2L_BSO) {
    1:  sw_livery_BR_Mk2L_BSO;
    1:  sw_livery_BR_Mk2L_BSO_REV; }

// position switch
switch(FEAT_TRAINS, SELF, sw_check_position_BR_Mk2L_BSO, position_in_consist) {
    1..7:   sw_livery_BR_Mk2L_BSO;
            random_choose_reverse_BR_Mk2L_BSO; }

// reverse switch
switch(FEAT_TRAINS, SELF, sw_check_reverse_BR_Mk2L_BSO, num_vehs_in_consist >= 6 ? position_in_consist_from_end : -1) {
    1:  sw_livery_BR_Mk2L_BSO_REV;
        sw_check_position_BR_Mk2L_BSO; }

// graphics switch
switch(FEAT_TRAINS, SELF, sw_graphics_BR_Mk2L_BSO, position_in_articulated_veh % 3) {
	1: return sw_check_reverse_BR_Mk2L_BSO;
	    spriteset_empty; }

// capacity switch
switch(FEAT_TRAINS, SELF, sw_capacity_BR_Mk2L_BSO, position_in_vehid_chain % 3) {
    0: 30 * param_pax; // all the capacity - (vehid chain number)
    1: 1 * param_pax;
    2: 42 * param_freight;
    // total 31 pax
}

// articulation switch
switch(FEAT_TRAINS, SELF, sw_articulated_BR_Mk2L_BSO, extra_callback_info1) {
	1:  return item_BR_Mk2L_BSO;
    2:  return item_BR_Mk2_B;
		return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}


