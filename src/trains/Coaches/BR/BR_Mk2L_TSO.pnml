switch(FEAT_TRAINS, SELF, sw_BR_Mk2L_TSO_cargo_subtype_text, cargo_subtype){
	0:	return string(str_BRBlueGrey);
    1:  return string(str_BRInterCity);
    2:  return string(str_BRScotRail);
    3:  return string(str_Virgin);
    4:  return string(str_Anglia);
    5:  return string(str_FGWFag);
    6:  return string(str_NetworkRail);
    7:  return string(str_ScotRailSaltire);
       return CB_RESULT_NO_TEXT; }

