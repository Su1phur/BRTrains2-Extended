    0:  sw_animation_GWR_3100;
        sw_animation_GWR_3100_REV; }

switch(FEAT_TRAINS, SELF, sw_spritestack_GWR_3100_purchase, [ STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8) ] ) {
    0: spriteset_GWR_3100_GWR_Green_Chrome_purchase;
    1: spriteset_GWR_3100_Anim_purchase; }

// FX effects 
switch(FEAT_TRAINS, SELF, sw_FX_GWR_3100_Steam, position_in_vehid_chain % 3) {
    0: return visual_effect_and_powered(VISUAL_EFFECT_STEAM, -2, DISABLE_WAGON_POWER);
    return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER); }

// livery switch
switch(FEAT_TRAINS, SELF, sw_livery_GWR_3100, cargo_subtype) {
    0: spriteset_GWR_3100_GWR_Green_Chrome;
    1: spriteset_GWR_3100_GWR_Green;
    2: spriteset_GWR_3100_GWR_Black;
    3: spriteset_GWR_3100_BR_Green_Early_Emblem;
    4: spriteset_GWR_3100_BR_Green_Late_Crest;
    5: spriteset_GWR_3100_BR_Black_Early_Emblem;
    6: spriteset_GWR_3100_BR_Black_Late_Crest; }

// livery name refit switch
switch(FEAT_TRAINS, SELF, sw_GWR_3100_cargo_subtype_text, cargo_subtype){
	0:	return string(str_GWRGreenChrome);
    1:  return string(str_GWRGreen);
    2:  return string(str_GWRBlack);
    3:  return string(str_BRGreen_Early);
    4:  return string(str_BRGreen_Late);
    5:  return string(str_BRPlainBlack_Early);
    6:  return string(str_BRPlainBlack_Late);
       return CB_RESULT_NO_TEXT; }

// Animation sprite stacking
switch(FEAT_TRAINS, SELF, sw_spritestack_GWR_3100, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
    0: sw_livery_GWR_3100;
    1: return sw_pick_animation_GWR_3100; }

// speed switch
switch(FEAT_TRAINS, SELF, sw_speed_GWR_3100, param_max_speed) {
    0: 50; // operational speed
    1: 75; // design speed
}

// year name switch
switch(FEAT_TRAINS, PARENT, sw_GWR_3100_name, current_year) {
    1903..1927: string(STR_NAME_GWR_3100);
    string(STR_NAME_GWR_3100_5100); }

// unit definition
item (FEAT_TRAINS, item_GWR_3100, 310) {
    property {
        // common properties
        name:                           string(STR_NAME_GWR_3100);
        // variant_group:                  ; // unit ID or item
		extra_flags:                    bitmask(VEHICLE_FLAG_SYNC_VARIANT_RELIABILITY);
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(1903,01,01); 
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life: 					40;
        retire_early: 					-10;							
        reliability_decay: 				20;
        
		// refittable_cargo_classes:        bitmask(CC_PASSENGERS);
        // non_refittable_cargo_classes:    bitmask();
        cargo_allow_refit:               [PASS, GOOD];
        // cargo_disallow_refit:            [LVST];
        misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 1;
        loading_speed:				    5; // All freight / Locomotive
        cost_factor: 					108;
        running_cost_base:			    RUNNING_COST_STEAM;					
        running_cost_factor: 			90;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          50 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        power:                          1370 hp;
        weight:                         77 ton;
        engine_class:                   ENGINE_CLASS_STEAM;
        dual_headed:                    0;
        air_drag_coefficient:           0.16;
        tractive_effort_coefficient:    0.15;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_spritestack_GWR_3100;
        visual_effect_and_powered:      sw_FX_GWR_3100_Steam;
        purchase:                       sw_spritestack_GWR_3100_purchase; // For dual mode units
        sound_effect:                   sw_sound_steam_generic;

        // internal code
        name:                           sw_GWR_3100_name;
        speed:                          sw_speed_GWR_3100() * 1609 / 1000;  
        purchase_cargo_capacity:        0;
        cargo_capacity:                 return 0;
        cost_factor:                    57;
        running_cost_factor:            3798;
        cargo_subtype_text:             sw_GWR_3100_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;
        // colour_mapping:                 switch_generic_recolour;

        // purchase menu text
        additional_text:                return( string(str_purchase_loco_with_liveries,
                                                    string(str_purchase_type_steam),
                                                    string(str_usage_mixed_traffic),
                                                    string(str_GWR_Large_Prairie_liveries)));
    }
}


