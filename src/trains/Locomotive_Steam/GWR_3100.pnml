        // cargo_disallow_refit:            [LVST];
        misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 1;
        loading_speed:				    5; // All freight / Locomotive
        cost_factor: 					108;
        running_cost_base:			    RUNNING_COST_STEAM;					
        running_cost_factor: 			90;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          60 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        power:                          1375 hp;
        weight:                         83 ton;
        engine_class:                   ENGINE_CLASS_STEAM;
        dual_headed:                    0;
        air_drag_coefficient:           0.16;
        tractive_effort_coefficient:    0.14;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_spritestack_GWR_5101;
        visual_effect_and_powered:      sw_FX_GWR_3100_Steam;
        purchase:                       sw_spritestack_GWR_5101_purchase; // For dual mode units
        sound_effect:                   sw_sound_steam_generic;

        // internal code
        name:                           sw_GWR_3150_name;
        speed:                          sw_speed_GWR_3100() * 1609 / 1000;  
        purchase_cargo_capacity:        0;
        cargo_capacity:                 return 0;
        cost_factor:                    58;
        running_cost_factor:            3808;
        cargo_subtype_text:             sw_GWR_5101_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;
        // colour_mapping:                 switch_generic_recolour;

        // purchase menu text
        additional_text:                return( string(str_purchase_loco_with_liveries,
                                                    string(str_purchase_type_steam),
                                                    string(str_usage_mixed_traffic),
                                                    string(str_GWR_Large_Prairie_liveries)));
    }
}

