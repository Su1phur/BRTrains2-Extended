// unit definition
item (FEAT_TRAINS, item_BR_BAA, 12263) {
    property {
        // common properties
        name:                           string(STR_NAME_BR_BAA);
        // variant_group:                  ; // unit ID or item
		// extra_flags:                    bitmask(VEHICLE_FLAG_DISABLE_NEW_VEHICLE_MESSAGE, VEHICLE_FLAG_DISABLE_EXCLUSIVE_PREVIEW, VEHICLE_FLAG_SYNC_VARIANT_EXCLUSIVE_PREVIEW, VEHICLE_FLAG_SYNC_VARIANT_RELIABILITY);
        climates_available:             ALL_CLIMATES;
        introduction_date:              date(1972,01,01); 
        model_life:                     VEHICLE_NEVER_EXPIRES;
        vehicle_life: 					40;
        retire_early: 					-10;							
        reliability_decay: 				20;
        
        refittable_cargo_classes:       bitmask(CC_OVERSIZED, CC_PIECE_GOODS);	
        non_refittable_cargo_classes:   bitmask(CC_POTABLE, CC_REFRIGERATED, CC_LIQUID, CC_HAZARDOUS);
        // cargo_allow_refit:               [MAIL, GOOD, VALU];
        cargo_disallow_refit:            [LVST];
        misc_flags: 					bitmask(TRAIN_FLAG_AUTOREFIT, TRAIN_FLAG_SPRITE_STACK);
        cargo_capacity:                 45 * param_freight;
        loading_speed:				    5; // All freight / Locomotive
        cost_factor: 					108;
        running_cost_base:			    RUNNING_COST_STEAM;					
        running_cost_factor: 			90;

        //specific properties
        sprite_id:                      SPRITE_ID_NEW_TRAIN;
        speed:                          60 mph;
        refit_cost:                     0;
        track_type:                     RAIL;
        weight:                         23 ton;
        dual_headed:                    0;
        length:                         8;
    }

    graphics {
        // graphics
        default:                        sw_loadstack_BR_BAA;
        purchase:                       spriteset_BR_BAA_BR_Bauxite_purchase;

        // internal code
        speed:                          sw_speed_BR_BAA() * 1609 / 1000;  
        cost_factor:                    921;
        running_cost_factor:            1238;
        cargo_subtype_text:             sw_BR_BAA_cargo_subtype_text; 
        colour_mapping:                 return PALETTE_CC_FIRST;

        // purchase menu text
        additional_text:                return( string(str_purchase_wagon,
                                                    string(str_BR_BAA_liveries)));
    }
}


